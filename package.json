{
    "name": "dotnet-core-commands",
    "displayName": "dotnet-commands",
    "description": "Extention for running dotnet SKD commands, EntityFramework migrations and managing NuGet Packages",
    "version": "1.0.0",
    "publisher": "Matija Ram≈°ak",
    "engines": {
        "vscode": "^1.20.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.Dotnet"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.Dotnet",
                "title": "dotnet"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.7.1",
        "vscode": "^1.1.10",
        "@types/node": "^9.4.5",
        "@types/mocha": "^2.2.48"
    },
    "dependencies": {
        "@types/xml-js": "^1.0.0",
        "file-system": "^2.2.2",
        "run-in-terminal": "^0.0.3",
        "web-request": "^1.0.7",
        "xml-js": "^1.6.2"
    }
}
